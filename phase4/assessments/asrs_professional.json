  "scoring": {
    "method": "weighted_composite",
    "formula": "(core_adhd * 0.4) + (workplace_impact * 0.3) + (life_management * 0.2) + (masking_indicators * 0.1)",
    "max_score": 100,
    "interpretation": {
      "0-25": {
        "level": "Low Risk",
        "description": "Minimal ADHD symptoms. Monitor and consider lifestyle interventions.",
        "recommendations": ["Stress management", "Sleep hygiene", "Regular exercise"]
      },
      "26-50": {
        "level": "Moderate Risk",
        "description": "Some ADHD symptoms present. Professional screening recommended.",
        "recommendations": ["Professional evaluation", "Workplace accommodations", "Time management strategies"]
      },
      "51-75": {
        "level": "High Risk",
        "description": "Significant ADHD symptoms. Comprehensive evaluation needed.",
        "recommendations": ["Comprehensive ADHD evaluation", "Consider medication consultation", "Therapy/coaching"]
      },
      "76-100": {
        "level": "Very High Risk",
        "description": "Severe ADHD symptoms. Urgent professional evaluation recommended.",
        "recommendations": ["Urgent professional evaluation", "Comprehensive treatment plan", "Workplace accommodations"]
      }
    }
  },
  "professional_modifiers": {
    "high_demand_career": {
      "roles": ["Executive", "Entrepreneur", "Consultant", "Sales"],
      "modifier": 5,
      "description": "High-pressure roles may exacerbate symptoms"
    },
    "detail_oriented_role": {
      "roles": ["Accountant", "Lawyer", "Doctor", "Engineer"],
      "modifier": 3,
      "description": "Detail-focused roles may highlight attention difficulties"
    },
    "creative_field": {
      "roles": ["Designer", "Writer", "Artist", "Marketing"],
      "modifier": -2,
      "description": "Creative environments may mask some symptoms"
    },
    "structured_environment": {
      "roles": ["Government", "Military", "Manufacturing"],
      "modifier": -3,
      "description": "Highly structured roles may provide external organization"
    }
  },
  "validation": {
    "evidence_base": "303 research entries from systematic literature review",
    "target_population_relevance": "95%+ professional adult males (25-65)",
    "evidence_quality": "90%+ high-quality sources (systematic reviews, RCTs)",
    "last_updated": "2025-09-12"
  }
}

