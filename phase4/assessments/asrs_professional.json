{
  "assessment_id": "asrs_professional_v1",
  "name": "ASRS-Based Professional ADHD Assessment",
  "description": "Evidence-based self-assessment tool for professional adult males (25-65) based on ASRS-v1.1 and validated research",
  "target_population": "Professional, intelligent, working-age men (25-65) with potential or diagnosed ADHD",
  "evidence_base": "Based on 44 assessment-related entries from 303-entry knowledge base",
